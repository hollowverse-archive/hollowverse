    <Block
      key="7bbc711c-174e-48fc-8e42-eac3dbdc71a6"
      node={
        Object {
          "id": "7bbc711c-174e-48fc-8e42-eac3dbdc71a6",
          "parentId": null,
          "sourceTitle": null,
          "sourceUrl": null,
