{
  "rules": [
    {
      "validation": "camelCase",
      "patterns": ["**/*"]
    },
    {
      "validation": "PascalCase",
      "patterns": [
        "src/app/components/**",
        "src/app/pages/**",
        "src/app/hocs/**"
      ]
    },
    {
      "validation": "ignore",
      "patterns": [
        "src/app/assets/algolia/*",
        "Dockerfile*",
        "_*.scss",
        "src/app/pages/NotablePerson/warningIcon.tsx"
      ]
    }
  ]
}
